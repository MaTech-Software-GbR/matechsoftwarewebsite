{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended",
    "standard-with-typescript",
    "plugin:import/typescript",
    "plugin:jest-extended/all",
    "airbnb",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks", "prettier", "jest-extended"],
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": "off",
    "prettier/prettier": ["error"]
  }
}
